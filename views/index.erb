<!doctype html>
<html>
  <head>
    <title>"CMS"</title>
  </head>

  <body>
    
    <% if session[:message] %>
      <p><%= session.delete(:message) %></p>
    <% end %>

    <ul>
      <% @files.each do |file| %>
        <li><a href="/<%= file %>"><%= file %></li>
      <% end %>
    </ul>

  </body>
</html>